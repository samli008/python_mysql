# ansibel model view
ansible-doc -l

# setup and ping model
ansible mfs -m setup
ansible mfs -m ping

# file model
ansible mfs -m file -a "src=/etc/fstab dest=/root/fstab state=link" #create soft link
ansible mfs -a 'ls -l /root'
ansible mfs -m file -a "path=/root/fstab state=absent" #delete file




